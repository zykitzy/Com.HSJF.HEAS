                        @GetDictionary("-EmergencyType")
                    </select>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>是否共同借款人</label>
                    <select class="form-control" data-field="IsCoBorrower" data-class="RelationPersonAudits" disabled>
                        <option value=""></option>
                        <option value="1">是</option>
                        <option value="0">否</option>
