                    <label>姓名</label>
                    <input id="person-name" class="form-control" type="text" data-field="Name" data-class="RelationPersonAudits" maxlength="100" disabled />
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>关系人类型</label>
                    <select id="person-type" class="form-control" data-fill-code="Dict:-PersonType" data-field="RelationType" onchange="guanxiren(this)" data-class="RelationPersonAudits" disabled>
                        <option></option>
                        @GetDictionary("-PersonType")
                    </select>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>与借款人关系</label>
                    <select id="relation-type" class="form-control" data-fill-code="Dict:-EmergencyType" data-field="BorrowerRelation" data-class="RelationPersonAudits" disabled>
