                    <select id="person-doctype" class="form-control" data-fill-code="Dict:-DocType" data-field="IdentificationType" data-class="RelationPersonAudits" onchange="IDType_Changed(this, 'person-docnumber', 'person-birthday');" action="onchange" disabled>
                        <option></option>
                        @GetDictionary("-DocType")
                    </select>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>证件号码</label>
                    <input id="person-docnumber" class="form-control" type="text" data-field="IdentificationNumber" data-class="RelationPersonAudits" maxlength="100" disabled />
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>证件有效期</label>
                    <div class="input-group">
                        <input class="form-control datePickerLimit" type="text" data-field="ExpiryDate" data-class="RelationPersonAudits" valid-type="date" disabled />
                        <span class="input-group-addon">
                            <i class="icon-calendar bigger-110"></i>
                        </span>
                        <span class="input-group-addon">
                            <input type="checkbox" name="Long-term" id="Long-term" value="1" data-field="" onclick="longterm(this);" disabled />&nbsp;&nbsp;长期
                        </span>
                    </div>
                </div>
                <div class="col-lg-4 col-md-6 col-sm-12 col-xs-12 form-group">
                    <label>证件复印件</label>
                    <label class="form-control control-label hs-clickable text-center" onclick="fileUploadFunction(this);" disabled>查看附件 &nbsp;<span class="blue">(0)</span></label>
