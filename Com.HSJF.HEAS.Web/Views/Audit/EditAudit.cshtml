<script type="text/javascript" src="../../js/init.js"></script>
<script type="text/javascript">
    //案件模式
    function caseModel(obj) {
        if ($(obj).val() != "-CaseMode-ZiYouZiJin") {
            $('[data-field="ThirdParty"]').removeAttr("disabled");
        } else {
            $('[data-field="ThirdParty"]').val("").attr("disabled", true);
        }
    }

    //保存
    function EditAuditCase() {
        //$("#shenpiyijian").find('[data-field="LoanDetailReportFile"]').removeAttr("data-field").removeAttr("data-class");
        //$("#shenpiyijian").find('[data-field="CaseDetail"]').removeAttr("data-field").removeAttr("data-class");
        if (ValidateCheckSave(".page-content")) {
            var data_case = GetJsonData("BaseAudit");
            data_case = checkSelectNull(data_case);
            selAccount();
            AjaxMethod("post", "/Audit/EditBaseAudit", data_case, function (r) {
                showAlert("保存成功");
                hideLoading();
                //          window.location = "/Audit/AuditIndex?status=ss";
            }, true);
        }
    }

    //自动保存
    function AutoSave() {
        var isValid = ValidateCheckSave(".page-content");
        var data_case = GetJsonData("BaseAudit");
        data_case = checkSelectNull(data_case);
        selAccount();
        if (isValid) {
            AjaxMethod("post", "/Audit/EditBaseAudit", data_case, function (r) { 
            	showAlert("保存成功"); 
            	hideLoading();
            }, true);
        }
    }
    //删除方法
    function deleteTr() {
        var isValid = ValidateCheckSave(".page-content");
        var data_case = GetJsonData("BaseCase");
        data_case = checkSelectNull(data_case);
        selAccount()
        if (isValid) {
            AjaxMethod("post", "/Audit/EditBaseAudit", data_case, function (r) {
                showAlert("删除成功");
                hideLoading();
            }, true);
        }
    }
    //提交
    function SubmitAuditCase() {
        //$("#shenpiyijian").find('[data-field="LoanDetailReportFile"]').removeAttr("data-field").removeAttr("data-class");
        //$("#shenpiyijian").find('[data-field="CaseDetail"]').removeAttr("data-field").removeAttr("data-class");
        
        /*********************房产信息必填******************************/
        var isSubfc = false;
        var isSubgxr = false;
        var isSubzx = false;
        var housueDetial = false;
         /*********************关系人 (*至少填写一个借款人)******************************/
        if ($("#basecase_person_list").find("tr").length > 1) {
            if (ValidateCheck(".page-content")) {
                var result = false;
                for (var i = 0; i < $('[data-field="RelationTypeText"]').length; i++) {
                    if ($('[data-field="RelationTypeText"]').eq(i).html() == "借款人") {
                        result = true;
                        break;
                    }
                }
                if (!result) {
                    showAlert("关系人-借款人 不能为空", "error");
                    $('[href="#person"]').click();
                    $("#personinfo").click();
                    return false;
                } else {
                    isSubgxr = true;
                }
            }
        } else {
            showAlert("关系人-借款人 不能为空", "error");
            $('[href="#person"]').click();
            $("#personinfo").click();
            return false;
        }
        if ($("#basecase_facility_list").find("tr").length > 1) {
            if (ValidateCheck(".page-content")) {
                var result = false;
                for (var i = 0; i < $('[data-field="CollateralTypeText"]').length; i++) {
                    if ($('[data-field="CollateralTypeText"]').eq(i).html() == "抵押物") {
                        result = true;
                        break;
                    }
                }
                if (!result) {
                    showAlert("房产信息-抵押物 不能为空", "error");
                    $('[href="#facility"]').click();
                    $("#fangchaninfo").click();
                    return false;
                } else {
                    isSubfc = true;
                }
            }
        } else {
            showAlert("房产信息-抵押物 不能为空", "error");
            $('[href="#facility"]').click();
            $("#fangchaninfo").click();
            return false;
        }
        
        
        /*********************所有抵押物必须有房屋明细******************************/
		var houseIdArr = [];
		var houseCount = 0;
		if ($("#audit_HouseDetails_list").find("tr").length>1) {
			 if (ValidateCheck(".page-content")) {
	             $("#basecase_facility_list").children("tbody").children("tr").each(function () {
					if ( $(this).children('[data-field="CollateralTypeText"]').html()=='抵押物') {
						houseIdArr.push($(this).children('[data-field="ID"]').html());
//						房屋明细中的tr 与之进行比较,有的话houseCount加1;						
						for (var i = 0; i < $("#audit_HouseDetails_list").children("tbody").find("tr").length; i++) {
							if ($("#audit_HouseDetails_list").children("tbody").find("tr").eq(i).children('[data-field="CollateralID"][data-class="HouseDetails"]').html() == $(this).children('[data-field="ID"]').html()) {
								houseCount++;
								break;
							}
						}
						
			       	}
			    });
			    if(houseCount != houseIdArr.length){
			        showAlert("所有的抵押物必须有房屋明细", "error");
			        $('[href="#researchInfo"]').click();
			        $("#HouseDetailsinfo").click();
			    }else{
			    	housueDetial = true;
			    }			
            }
		}else{
		    showAlert("所有的抵押物必须有房屋明细", "error");
		    $('[href="#researchInfo"]').click();
		    $("#HouseDetailsinfo").click();
		}

        /*********************个人资信情况(*至少填写借款人的资信情况)******************************/
		if(housueDetial){
		    if ($("#audit_IndividualCredits_list").find("tr").length > 1) {
		        if (ValidateCheck(".page-content")) {
		            var result = false;
		            //两层循环
		            for (var i = 0; i < $('[data-class="RelationPersonAudits"][data-field="ID"]').length; i++) {
		                if ($('[data-class="RelationPersonAudits"][data-field="RelationTypeText"]').eq(i).html() == "借款人") {
		                    for (var j = 0; j < $('[data-class="IndividualCredits"][data-field="PersonID"]').length; j++) {
		                        if ($('[data-class="RelationPersonAudits"][data-field="ID"]').eq(i).html() == $('[data-class="IndividualCredits"][data-field="PersonID"]').eq(j).html()) {
		                            result = true;
		                            break;
		                        }
		                    }
		                }
		            }
		            if (!result) {
		                showAlert("个人资信情况-借款人 不能为空", "error");
		                $('[href="#AuditInfo"]').click();
		                $("#IndividualCreditsinfo").click();
		            } else {
		                isSubzx = true;
		            }
		        }
		    } else {
		        showAlert("个人资信情况-借款人 不能为空", "error");
		        $('[href="#AuditInfo"]').click();
		        $("#IndividualCreditsinfo").click();
		    }
		}

        if (isSubfc && isSubgxr && isSubzx && housueDetial) {
            ConfirmBox('<div>是否确认提交?</div><br><textarea rows="5" cols="38" width="100%" placeholder="备注" id="des"></textarea>', function () {
                $('#BaseAudit_Description').val($("#des").val());
                var data_case = GetJsonData("BaseAudit", $(window.document));
                data_case = checkSelectNull(data_case);
                selAccount();
                AjaxMethod("post", "/Audit/SubmitBaseAudit", data_case, function (r) {
                    MessageBox("提交成功", function () {
                        window.close();
                    });
                    hideLoading();
                }, true);
            });
        }
    }

